{
  "title": "MongoDB Operations",
  "uid": "mongodb-ops",
  "tags": ["cdc", "mongodb", "k6"],
  "timezone": "browser",
  "refresh": "10s",
  "schemaVersion": 39,
  "panels": [
    {
      "id": 1,
      "title": "Connection Pool Size",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "mongodb_driver_pool_size",
          "legendFormat": "Pool Size",
          "refId": "A"
        },
        {
          "expr": "mongodb_driver_pool_checkedout",
          "legendFormat": "Checked Out",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10,
            "lineWidth": 1,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "mode": "none", "group": "A" },
            "axisPlacement": "auto",
            "axisBorderShow": false,
            "scaleDistribution": { "type": "linear" }
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "id": 2,
      "title": "Connection Pool Wait Queue",
      "type": "timeseries",
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "mongodb_driver_pool_waitqueuesize",
          "legendFormat": "Wait Queue Size",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10,
            "lineWidth": 1,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "mode": "none", "group": "A" },
            "axisPlacement": "auto",
            "axisBorderShow": false,
            "scaleDistribution": { "type": "linear" }
          },
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "id": 3,
      "title": "Checkout Failures",
      "type": "stat",
      "gridPos": { "x": 16, "y": 0, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "increase(mongodb_driver_pool_checkoutfailed_total[1h])",
          "legendFormat": "Failures (1h)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto"
      }
    },
    {
      "id": 4,
      "title": "k6: MongoDB Read Latency",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "k6_mongo_read_duration_p50",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "k6_mongo_read_duration_p95",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "k6_mongo_read_duration_p99",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 0,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "mode": "none", "group": "A" },
            "axisPlacement": "auto",
            "axisBorderShow": false,
            "scaleDistribution": { "type": "linear" }
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "id": 5,
      "title": "k6: Documents Found",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(k6_mongo_documents_found_total[1m])",
          "legendFormat": "Documents/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10,
            "lineWidth": 1,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "mode": "none", "group": "A" },
            "axisPlacement": "auto",
            "axisBorderShow": false,
            "scaleDistribution": { "type": "linear" }
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "id": 6,
      "title": "k6: CDC Success Rate",
      "type": "gauge",
      "gridPos": { "x": 0, "y": 16, "w": 8, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "k6_cdc_success_rate_rate",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 0.90 },
              { "color": "green", "value": 0.95 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 7,
      "title": "k6: CDC E2E Latency",
      "type": "timeseries",
      "gridPos": { "x": 8, "y": 16, "w": 16, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "k6_cdc_e2e_latency_p50",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "k6_cdc_e2e_latency_p95",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "k6_cdc_e2e_latency_p99",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 0,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "mode": "none", "group": "A" },
            "axisPlacement": "auto",
            "axisBorderShow": false,
            "scaleDistribution": { "type": "linear" }
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "version": 2
}
